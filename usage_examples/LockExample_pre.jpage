import lombok.ReadLock;
import lombok.Validate.NotEmpty;
import lombok.Validate.NotNull;
import lombok.WriteLock;

import java.util.HashMap;
import java.util.Map;

class LockExample {
	private Map<String, String> dictionary = new HashMap<String, String>();
	
	@WriteLock("dictionaryLock")
	public void put(@NotEmpty String key, @NotNull String value) {
		dictionary.put(key, value);
	}
	
	@ReadLock("dictionaryLock")
	public String get(@NotEmpty String key) {
		return dictionary.get(key);
	}
}