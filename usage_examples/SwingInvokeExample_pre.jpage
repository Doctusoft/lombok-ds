import lombok.SwingInvokeLater;
import lombok.SwingInvokeLater;

import javax.swing.JFrame;

class SwingInvokeLaterExample {

	@SwingInvokeLater
	void createUI() {
		JFrame frame = new JFrame()
		frame.setTitle("Application");
		frame.setVisible(true);
	}
}

class SwingInvokeAndWaitExample {

	@SwingInvokeAndWait
	void createUI() {
		JFrame frame = new JFrame()
		frame.setTitle("Application");
		frame.setVisible(true);
	}
}